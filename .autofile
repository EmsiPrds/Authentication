.init {
  $ taskkill /F /IM node.exe
  $ code .
}
dev {
  $ wr http://localhost:3000
  + cd auth-backend && npm run dev
  + cd auth-frontend && npm run dev
}

build {
  + cd auth-backend && npm run build
  + cd auth-frontend && npm run build

}

publish {
  + cd auth-backend && npm run publish
  + cd auth-frontend && npm run publish

}


share {
  + sshtunnel
  + code serve-web --host 0.0.0.0 --port 5000 --without-connection-token --accept-server-license-terms
}